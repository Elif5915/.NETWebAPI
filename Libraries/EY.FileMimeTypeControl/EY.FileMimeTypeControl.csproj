<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net8.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <GeneratePackageOnBuild>True</GeneratePackageOnBuild>
    <PackageId>EY.FileMimeTypeControl</PackageId>
    <Version>1.0.0</Version>
    <Authors>Elif Yıldırım</Authors>
    <Company>Elif Yıldırım</Company>
    <Product>EY.FileMimeTypeControl</Product>
    <Description>Bu paket ile dosyalarınızın type kontrolünü kolayca yapabilirsiniz.</Description>
    <PackageReadmeFile>README.md</PackageReadmeFile>
    <RepositoryUrl>https://github.com/Elif5915/.NETWebAPI</RepositoryUrl>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.AspNetCore.Http.Features" Version="5.0.17" />
  </ItemGroup>

  <ItemGroup>
    <None Update="README.md">
      <Pack>True</Pack>
      <PackagePath>\</PackagePath>
    </None>
  </ItemGroup>

</Project>
